{
  "name": "download-central",
  "version": "1.0.3",
  "description": "Download central is a windows app to download other desktop apps and files",
  "author": "ASHES SOFTWARE",
  "main": "src/main.js",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.75",
    "@electron-forge/maker-deb": "^6.0.3",
    "@electron-forge/maker-rpm": "^6.0.3",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.3",
    "electron": "^21.3.1",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^17.1.1",
    "electron-wix-msi": "^5.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "electron-squirrel-startup": "^1.0.0"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/",
    "make": "electron-forge make",
    "setup": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "download_central"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
